{
  "name": "Spacemacs - dark",
  "colors": {
    "debugToolBar.background": "#292b2e",
    "activityBar.background": "#292b2e",
    "activityBar.foreground": "#c6c6c6",
    "activityBarBadge.background": "#4f97d7",
    "activityBarBadge.foreground": "#292b2e",
    "contrastBorder": "#544a65",
    "dropdown.background": "#292b2e",
    "dropdown.border": "#544a65",
    "dropdown.foreground": "#c6c6c6",
    "editor.background": "#292b2e",
    "editor.foreground": "#c6c6c6",
    "editor.inactiveSelectionBackground": "#303030",
    "editor.lineHighlightBackground": "#303030",
    "editor.selectionBackground": "#4f97d7",
    "editor.selectionHighlightBackground": "#303030",
    "editorBracketMatch.background": "#4f97d7",
    "editorBracketMatch.border": "#4f97d7",
    "editorCursor.foreground": "#d1951d",
    "editorGroupHeader.noTabsBackground": "#292b2e",
    "editorGroupHeader.tabsBackground": "#292b2e",
    "editorIndentGuide.background": "#303030",
    "editorLineNumber.foreground": "#44505c",
    "editorLineNumber.activeForeground": "#FF62B0",
    "list.activeSelectionBackground": "#4f97d7",
    "list.activeSelectionForeground": "#292b2e",
    "list.dropBackground": "#303030",
    "list.focusBackground": "#303030",
    "list.highlightForeground": "#4f97d7",
    "list.hoverBackground": "#303030",
    "list.inactiveSelectionBackground": "#303030",
    "notification.background": "#34323e",
    "notification.foreground": "#c6c6c6",
    "sideBar.background": "#292b2e",
    "sideBarSectionHeader.background": "#303030",
    "sideBarTitle.foreground": "#c6c6c6",
    "statusBar.background": "#b888e2",
    "statusBar.debuggingBackground": "#ce537a",
    "statusBar.foreground": "#292b2e",
    "statusBar.noFolderBackground": "#666666",
    "statusBarItem.activeBackground": "#4f97d7",
    "statusBarItem.hoverBackground": "#4f97d7",
    "statusBarItem.prominentBackground": "#4f97d7",
    "statusBarItem.prominentHoverBackground": "#4f97d7",
    "tab.activeBackground": "#d1951d",
    "tab.activeForeground": "#292b2e",
    "tab.border": "#544a65",
    "tab.inactiveBackground": "#544a65",
    "tab.inactiveForeground": "#a8a8a8",
    "terminal.ansiBlack": "#292b2e",
    "terminal.ansiBlue": "#4f97d7",
    "terminal.ansiBrightBlack": "#544a65",
    "terminal.ansiBrightBlue": "#4f97d7",
    "terminal.ansiBrightCyan": "#28def0",
    "terminal.ansiBrightGreen": "#67b11d",
    "terminal.ansiBrightMagenta": "#bc6ec5",
    "terminal.ansiBrightRed": "#ce537a",
    "terminal.ansiBrightWhite": "#c6c6c6",
    "terminal.ansiBrightYellow": "#d1951d",
    "terminal.ansiCyan": "#20af81",
    "terminal.ansiGreen": "#67b11d",
    "terminal.ansiMagenta": "#bc6ec5",
    "terminal.ansiRed": "#ce537a",
    "terminal.ansiWhite": "#c6c6c6",
    "terminal.ansiYellow": "#d1951d",
    "widget.shadow": "#292b2e"
  },
  "tokenColors": [
    {
      "settings": {
        "background": "#292b2e",
        "foreground": "#c6c6c6"
      }
    },
    {
      "name": "Comments",
      "scope": "comment",
      "settings": {
        "foreground": "#65737e"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#20af81"
      }
    },
    {
      "name": "Numbers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#e697e6"
      }
    },
    {
      "name": "Built-in constants",
      "scope": "constant.language",
      "settings": {
        "foreground": "#e697e6"
      }
    },
    {
      "name": "User-defined constants",
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#e697e6"
      }
    },
    {
      "name": "Variables",
      "scope": "variable",
      "settings": {
        "foreground": "#ce537a",
        "fontStyle": ""
      }
    },
    {
      "name": "Keywords",
      "scope": "keyword",
      "settings": {
        "foreground": "#4f97d7",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#4f97d7"
      }
    },
    {
      "name": "Storage types",
      "scope": "storage.type",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#4f97d7"
      }
    },
    {
      "name": "Class names",
      "scope": [
        "entity.name.class",
        "entity.name.type"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#4f97d7"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#bc6ec5"
      }
    },
    {
      "name": "Function names",
      "scope": "entity.name.function",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#bc6ec5"
      }
    },
    {
      "name": "Function arguments",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "",
        "foreground": "#ce537a"
      }
    },
    {
      "name": "Tag names",
      "scope": "entity.name.tag",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#4f97d7"
      }
    },
    {
      "name": "Tag start/end",
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#4f97d7"
      }
    },
    {
      "name": "Tag attributes",
      "scope": "entity.other.attribute-name",
      "settings": {
        "fontStyle": "",
        "foreground": "#bc6ec5"
      }
    },
    {
      "name": "Library functions",
      "scope": "support.function",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#bc6ec5"
      }
    },
    {
      "name": "Library constants",
      "scope": "support.constant",
      "settings": {
        "fontStyle": "",
        "foreground": "#a45bad"
      }
    },
    {
      "name": "Library classes/types",
      "scope": [
        "support.type",
        "support.class"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ce537a"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#e0211d"
      }
    },
    {
      "name": "diff: header",
      "scope": [
        "meta.diff",
        "meta.diff.header"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#E0EDDD"
      }
    },
    {
      "name": "diff: deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#dc322f"
      }
    },
    {
      "name": "diff: changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#cb4b16"
      }
    },
    {
      "name": "diff: inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#219186"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#22aa44"
      }
    },
    {
      "name": "Markup Styling",
      "scope": [
        "markup.bold",
        "markup.italic"
      ],
      "settings": {
        "foreground": "#22aa44"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#9966b8"
      }
    },
    {
      "name": "Markup Setext Header",
      "scope": "markup.heading.setext",
      "settings": {
        "foreground": "#ddbb88"
      }
    },
    {
      "name": "Source Java Storage Modifier Import",
      "scope": [
        "source.java storage.modifier.import",
        "source.java storage.modifier.package",
        "source.java entity.name.type.class",
        "source.java storage.type",
        "source.java meta.definition.class.implemented.interfaces",
        "source.java entity.other.inherited-class"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#ce537a"
      }
    },
    {
      "name": "Java Method Body",
      "scope": [
        "meta.method.body.java",
        "meta.function-call.java"
      ],
      "settings": {
        "foreground": "#cbc1d5ff"
      }
    },
    {
      "name": "Java Operators",
      "scope": [
        "source.java keyword.operator.assignment",
        "source.java keyword.operator.arithmetic",
        "source.java storage.type.function.arrow.java",
        "source.java punctuation.definition.parameters.begin.bracket.round",
        "source.java punctuation.definition.parameters.end.bracket.round"
      ],
      "settings": {
        "foreground": "#cbc1d5ff"
      }
    },
    {
      "name": "Java Curly Braces",
      "scope": [
        "source.java begin.bracket.curly",
        "source.java end.bracket.curly"
      ],
      "settings": {
        "foreground": "#4f97d7"
      }
    },
    {
      "name": "Java Constructor",
      "scope": "source.java entity.name.function meta",
      "settings": {
        "foreground": "#ce537a"
      }
    },
    {
      "name": "Javadoc keywords",
      "scope": "source.java keyword.other.documentation.javadoc.java",
      "settings": {
        "foreground": "#9966b8"
      }
    },
    {
      "name": "C++ Directives",
      "scope": [
        "source.cpp keyword.control.directive"
      ],
      "settings": {
        "foreground": "#9966b8"
      }
    },
    {
      "name": "C++ Keywords",
      "scope": [
        "source.cpp storage.type",
        "source.cpp storage.modifier"
      ],
      "settings": {
        "foreground": "#4f97d7"
      }
    },
    {
      "name": "Clojure locals",
      "scope": "source.clojure meta.symbol",
      "settings": {
        "foreground": "#cbc1d5ff"
      }
    },
    {
      "name": "Clojure namespace",
      "scope": [
        "source.clojure entity.global",
        "source.clojure meta.definition.global"
      ],
      "settings": {
        "foreground": "#ce537a"
      }
    },
    {
      "name": "Clojure keyword",
      "scope": "source.clojure constant.keyword",
      "settings": {
        "foreground": "#9966b8"
      }
    },
    {
      "name": "Clojure namespace",
      "scope": [
        "source.clojure meta.symbol meta.expression",
        "source.clojure meta.vector"
      ],
      "settings": {
        "foreground": "#ce537a"
      }
    }
  ]
}
