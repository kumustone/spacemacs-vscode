{
  "name": "Spacemacs - light",
  "colors": {
    "editor.background": "#fbf8ef",
    "editor.foreground": "#655370",
    "editor.lineHighlightBackground": "#efeae9",
    "editor.selectionBackground": "#d3d3e7",
    "editor.inactiveSelectionBackground": "#e7e7fc",
    "editor.selectionHighlightBackground": "#e7e7fc",
    "editorCursor.foreground": "#504259",
    "editorLineNumber.foreground": "#a8a4ae",
    "editorLineNumber.activeForeground": "#6c3163",
    
    "activityBar.background": "#fbf8ef",
    "activityBar.foreground": "#655370",
    "activityBarBadge.background": "#3a81c3",
    "activityBarBadge.foreground": "#fbf8ef",
    
    "sideBar.background": "#fbf8ef",
    "sideBar.foreground": "#655370",
    "sideBarTitle.foreground": "#655370",
    "sideBarSectionHeader.background": "#efeae9",
    
    "statusBar.background": "#3a81c3",
    "statusBar.foreground": "#fbf8ef",
    "statusBar.debuggingBackground": "#ba2f59",
    "statusBar.noFolderBackground": "#504259",
    "statusBarItem.activeBackground": "#6c3163",
    "statusBarItem.hoverBackground": "#6c3163",
    
    "titleBar.activeBackground": "#fbf8ef",
    "titleBar.activeForeground": "#655370",
    
    "tab.activeBackground": "#fbf8ef",
    "tab.activeForeground": "#655370",
    "tab.inactiveBackground": "#efeae9",
    "tab.inactiveForeground": "#a8a4ae",
    "tab.border": "#b3b9be",
    
    "terminal.ansiBlack": "#fbf8ef",
    "terminal.ansiBlue": "#3a81c3",
    "terminal.ansiBrightBlack": "#a8a4ae",
    "terminal.ansiBrightBlue": "#3a81c3",
    "terminal.ansiBrightCyan": "#24775c",
    "terminal.ansiBrightGreen": "#67b11d",
    "terminal.ansiBrightMagenta": "#6c3163",
    "terminal.ansiBrightRed": "#f2241f",
    "terminal.ansiBrightWhite": "#655370",
    "terminal.ansiBrightYellow": "#b1951d",
    "terminal.ansiCyan": "#24775c",
    "terminal.ansiGreen": "#67b11d",
    "terminal.ansiMagenta": "#6c3163",
    "terminal.ansiRed": "#ba2f59",
    "terminal.ansiWhite": "#655370",
    "terminal.ansiYellow": "#b1951d",
    
    "list.activeSelectionBackground": "#d3d3e7",
    "list.activeSelectionForeground": "#655370",
    "list.inactiveSelectionBackground": "#efeae9",
    "list.hoverBackground": "#efeae9",
    "list.focusBackground": "#d3d3e7"
  },
  "tokenColors": [
    {
      "settings": {
        "background": "#fbf8ef",
        "foreground": "#655370"
      }
    },
    {
      "name": "Comments",
      "scope": "comment",
      "settings": {
        "foreground": "#2aa1ae",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#24775c"
      }
    },
    {
      "name": "Numbers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#6c3163"
      }
    },
    {
      "name": "Built-in constants",
      "scope": "constant.language",
      "settings": {
        "foreground": "#6c3163"
      }
    },
    {
      "name": "User-defined constants",
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#6c3163"
      }
    },
    {
      "name": "Variables",
      "scope": "variable",
      "settings": {
        "foreground": "#715ab1"
      }
    },
    {
      "name": "Keywords",
      "scope": "keyword",
      "settings": {
        "foreground": "#3a81c3",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#3a81c3",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage types",
      "scope": "storage.type",
      "settings": {
        "foreground": "#715ab1",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Class names",
      "scope": [
        "entity.name.class",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#3a81c3",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#3a81c3",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Function names",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#6c3163",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function arguments",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#715ab1"
      }
    },
    {
      "name": "Tag names",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#3a81c3",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Tag attributes",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#6c3163"
      }
    },
    {
      "name": "Library functions",
      "scope": "support.function",
      "settings": {
        "foreground": "#6c3163",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Library constants",
      "scope": "support.constant",
      "settings": {
        "foreground": "#6c3163"
      }
    },
    {
      "name": "Library classes/types",
      "scope": [
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#3a81c3",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#f2241f",
        "background": "#fbf8ef"
      }
    }
  ]
}
